[program:{{ application_name }}]
command =/home/{{ default_user }}/.virtualenvs/{{ application_name }}/bin/python /home/{{ default_user }}/{{ application_name }}/manage.py run_gunicorn -w 4 -b 127.0.0.1:8000
user = {{ default_user }}
stdout_logfile = {{ gunicorn_err_file }}
stderr_logfile = {{ gunicorn_log_file }}
